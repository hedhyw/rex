{
  "linters": {
    "enable-all": true,
    "disable": [
      "nonamedreturns",
      "wrapcheck",
      "varnamelen",
      "thelper",
      "paralleltest",
      "ireturn",
      "gomnd",
      "nolintlint",
      "depguard",
      "goconst",
      "maligned",
      "structcheck",
      "varcheck",
      "nosnakecase",
      "interfacer",
      "golint",
      "scopelint",
      "exhaustivestruct",
      "ifshort",
      "deadcode"
    ]
  },
  "linters-settings": {
    "goimports": {
      "local-prefixes": "github.com/hedhyw/rex/"
    },
    "revive": {}
  }
}